{
  "extends": "expo/tsconfig.base",
  "exclude": ["node_modules", ".next"],
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "types": ["jest", "node"],
    "target": "ES2020",
    "baseUrl": "./",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@core/*": ["./src/core/*"],
      "@context/*": ["./src/context/*"],
      "@modules/*": ["./src/modules/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@assets/*": ["./src/assets/*"],
      "@typings/*": ["src/typings/*"],
      "@navigation/*": ["src/navigation/*"],
      "@screens/*": ["src/screens/*"],
      "@store/*": ["src/store/*"]
    }
  }
}
