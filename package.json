    "@react-native-firebase/messaging": "^17.4.2",
    "@react-native-picker/picker": "2.4.8",
    "@react-navigation/bottom-tabs": "^6.5.3",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/native-stack": "^6.9.8",
    "@react-navigation/stack": "^6.3.12",
    "@reduxjs/toolkit": "^1.9.3",
    "add": "^2.0.6",
    "appcenter": "^4.4.5",
    "appcenter-analytics": "^4.4.5",
    "appcenter-crashes": "^4.4.5",
    "axios": "^1.3.1",
    "date-fns": "^2.29.3",
    "expo": "~47.0.12",
    "expo-av": "~13.0.2",
    "expo-build-properties": "~0.4.1",
    "expo-checkbox": "~2.2.2",
    "expo-clipboard": "~4.0.1",
    "expo-contacts": "~11.0.1",
    "expo-dev-client": "~2.0.1",
    "expo-document-picker": "~11.0.1",
    "expo-file-system": "~15.1.1",
    "expo-font": "~11.0.1",
    "expo-haptics": "~12.0.1",
    "expo-image-manipulator": "~11.0.0",
    "expo-image-picker": "~14.0.2",
    "expo-linear-gradient": "~12.0.1",
    "expo-media-library": "~15.0.0",
    "expo-sharing": "~11.0.1",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "firebase": "9.6.1",
    "formik": "^2.2.9",
    "jest": "^29",
    "jest-expo": "^48.0.1",
    "jwt-decode": "^3.1.2",
    "newrelic-react-native-agent": "1.2.1",
    "patch-package": "^6.5.1",
    "postinstall-postinstall": "^2.1.0",
    "prop-types": "^15.8.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-code-push": "7.0.0",
    "react-native-config": "^1.5.0",
    "react-native-date-picker": "^4.2.13",
    "react-native-error-boundary": "^1.2.3",
    "react-native-floating-label-input": "^1.4.2",
